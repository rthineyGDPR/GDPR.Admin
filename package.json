{
  "name": "safeguard-admin",
  "version": "1.0.0",
  "private": true,
  "main": "electron.main.js",
  "scripts": {
    "ng": "ng",
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --proxy-config proxy.conf.js",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "npm run env -s && ng test",
    "test:ci": "npm run env -s && npm run lint -s && ng test --configuration=ci",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "docs": "hads ./docs -o",
    "compodoc": "compodoc -p tsconfig.app.json -d docs/generated -s -o",
    "env": "ngx-scripts env npm_package_version",
    "prettier": "prettier --write \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src,e2e}/**/*.{ts,js,html,scss}\"",
    "postinstall": "npm run prettier -s",
    "electron:compile": "tsc --project tsconfig.electron.json",
    "electron:run": "npm run electron:compile -s && electron .",
    "electron:build": "npm run electron:compile -s && npm run env -s && ng build --prod --base-href ./dist.electron --output-path dist.electron",
    "electron:package:common": "electron-packager . --overwrite --out=dist.packages --ignore=\"/(platforms|plugins|docs|www|reports|e2e|src)\"",
    "electron:package:windows": "npm run electron:package:common -s -- --platform=win32 --arch=x64 --icon=./resources/win/icon.ico",
    "electron:package:mac": "npm run electron:package:common -s -- --platform=darwin --icon=./resources/mac/icon.icns",
    "electron:package:linux": "npm run electron:package:common -s --platform=linux",
    "electron:package": "npm run electron:package:windows -s && npm run electron:package:mac -s && npm run electron:package:linux -s",
    "generate": "ng generate",
    "deploy": "npm run build -s && ng run safeguard-admin:deploy",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:jest:ci": "jest --runInBand"
  },
  "dependencies": {
    "@angular/animations": "8.2.13",
    "@angular/cdk": "^8.0.2",
    "@angular/common": "8.2.13",
    "@angular/compiler": "8.2.13",
    "@angular/compiler-cli": "8.2.13",
    "@angular/core": "8.2.13",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "8.2.13",
    "@angular/material": "^8.0.2",
    "@angular/platform-browser": "8.2.13",
    "@angular/platform-browser-dynamic": "8.2.13",
    "@angular/platform-server": "8.2.13",
    "@angular/router": "8.2.13",
    "@azure/ng-deploy": "^0.2.3",
    "@microsoft/signalr": "3.0.0",
    "@microsoft/signalr-protocol-msgpack": "3.0.0",
    "@nebular/theme": "^4.5.0",
    "@ng-bootstrap/ng-bootstrap": "5.1.2",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "@ngx-translate/core": "^11.0.1",
    "@swimlane/ngx-datatable": "16.0.2",
    "@syncfusion/ej2": "^17.3.30",
    "@syncfusion/ej2-angular-buttons": "^17.3.27",
    "@syncfusion/ej2-angular-charts": "^17.3.34",
    "@syncfusion/ej2-angular-dropdowns": "^17.3.30",
    "@syncfusion/ej2-angular-filemanager": "^17.3.28",
    "@syncfusion/ej2-angular-inputs": "^17.3.29",
    "@syncfusion/ej2-angular-layouts": "^17.3.29",
    "@syncfusion/ej2-angular-navigations": "^17.3.27",
    "@syncfusion/ej2-angular-notifications": "^17.3.30",
    "@syncfusion/ej2-angular-pdfviewer": "^17.3.30",
    "@syncfusion/ej2-angular-pivotview": "^17.3.29",
    "@syncfusion/ej2-angular-popups": "^17.3.29",
    "@syncfusion/ej2-angular-richtexteditor": "^17.3.29",
    "@syncfusion/ej2-data": "^17.3.30",
    "@syncfusion/ej2-layouts": "^17.3.29",
    "angular-calendar": "0.27.20",
    "angularx-flatpickr": "6.1.1",
    "aspnet-prerendering": "3.0.1",
    "bootstrap": "4.3.1",
    "date-fns": "2.6.0",
    "flatpickr": "4.6.3",
    "font-awesome": "4.7.0",
    "g": "^2.0.1",
    "hammerjs": "^2.0.8",
    "lodash": "4.17.15",
    "material-design-icons-iconfont": "^5.0.1",
    "moment": "^2.24.0",
    "oidc-client": "1.9.1",
    "preboot": "7.0.0",
    "rxjs": "6.5.3",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-builders/jest": "^8.0.4",
    "@angular-devkit/build-angular": "~0.803.17",
    "@angular/cli": "^8.3.14",
    "@angular/compiler-cli": "^8.1.0",
    "@angular/language-service": "8.2.13",
    "@angular/pwa": "0.803.14",
    "@angular/service-worker": "8.2.13",
    "@angularclass/hmr": "^2.1.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@compodoc/compodoc": "1.1.11",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "^3.3.13",
    "@types/jasminewd2": "^2.0.3",
    "@types/jest": "^24.0.20",
    "@types/lodash": "^4.14.112",
    "@types/node": "12.11.7",
    "codelyzer": "^5.2.0",
    "electron": "^11.5.0",
    "electron-packager": "^13.0.1",
    "electron-reload": "^1.4.0",
    "hads": "^1.7.0",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^2.0.0",
    "husky": "^3.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^24.9.0",
    "jest-preset-angular": "^8.0.0",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "protractor": "5.4.2",
    "puppeteer": "^1.17.0",
    "stylelint": "~10.1.0",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended-scss": "~3.3.0",
    "stylelint-config-standard": "~18.3.0",
    "stylelint-scss": "~3.9.0",
    "testcafe": "^1.7.0",
    "testcafe-reporter-html": "^1.4.5",
    "testcafe-reporter-xunit": "^2.1.0",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "3.5.3",
    "webpack-bundle-analyzer": "3.6.0"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png)$": "<rootDir>/__mocks__/image.js",
      "@app/(.*)": "<rootDir>/src/app/$1",
      "assets/(.*)": "<rootDir>/src/assets/$1",
      "environments/(.*)": "<rootDir>/src/environments/$1"
    }
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
